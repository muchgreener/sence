<h3>Listing users</h3>

<table class="table table-condensed">
  <tr>
    <th>ID</th>
    <th>email</th>
    <th>apple_id</th>
    <th>create date</th>
    <th>update date</th>
    <th>last signin date</th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%= user.email %> </td>
    <td><%= user.apple_id %></td>
    <td><%= user.created_at %></td>
    <td><%= user.updated_at %></td>
    <td><%= user.last_sign_in_at %></td>
    <td><%= link_to 'Show Questions', user_questions_path(user.id) %></td>
  </tr>
<% end %>
</table>

<br />

