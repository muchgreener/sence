<h3>Listing users</h3>

<table class="table table-condensed">
  <tr>
    <th>ID</th>
    <th>email</th>
    <th>apple_id</th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%= user.email %> </td>
    <td><%= user.apple_id %></td>
    <td><%= link_to 'Show', user %></td>
  </tr>
<% end %>
</table>

<br />

